{
	"title": "Encondings for types supported by lisk-codec",
	"summary": "Examples of encoding all the types supported by lisk-codec",
	"config": {
		"network": "devnet"
	},
	"runner": "lisk_codec",
	"handler": "validEncodings",
	"testCases": [
		{
			"description": "Encoding 32 bit unsigned number",
			"input": {
				"object": {
					"number": 10
				},
				"schema": {
					"$id": "object1",
					"type": "object",
					"properties": {
						"number": {
							"dataType": "uint32",
							"fieldNumber": 1
						}
					}
				}
			},
			"output": {
				"value": "080a"
			}
		},
		{
			"description": "Encoding 32 bit signed number",
			"input": {
				"object": {
					"number": -10
				},
				"schema": {
					"$id": "object2",
					"type": "object",
					"properties": {
						"number": {
							"dataType": "sint32",
							"fieldNumber": 1
						}
					}
				}
			},
			"output": {
				"value": "0813"
			}
		},
		{
